<h2>Listing question_types</h2>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @question_types.each do |question_type| %>
  <tr>
    <td><%=h question_type.name %></td>
    <td><%= link_to 'Edit', edit_question_type_path(question_type) %></td>
    <td><%= link_to 'Destroy', question_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New question_type', new_question_type_path %>